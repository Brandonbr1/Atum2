{
  "multipart": [
    {   "when": { "down": "true" },
        "apply": { "model": "atum:block/deadwood_branch" }},
    {   "when": { "east": "true" },
        "apply": { "model": "atum:block/deadwood_branch", "x": -90, "y": 90 }},
    {   "when": { "north": "true" },
        "apply": { "model": "atum:block/deadwood_branch", "x": -90 }},
    {   "when": { "south": "true" },
        "apply": { "model": "atum:block/deadwood_branch", "x": 90 }},
    {   "when": { "up": "true" },
        "apply": { "model": "atum:block/deadwood_branch", "x": 180 }},
    {   "when": { "west": "true" },
        "apply": { "model": "atum:block/deadwood_branch", "x": 90, "y": 90 }},
    {   "when": { "facing": "north" },
        "apply": { "model": "atum:block/deadwood_stem", "x": -90 }},
    {   "when": { "facing": "south" },
        "apply": { "model": "atum:block/deadwood_stem", "x": 90 }},
    {   "when": { "facing": "east" },
        "apply": { "model": "atum:block/deadwood_stem", "x": -90, "y": 90 }},
    {   "when": { "facing": "west" },
        "apply": { "model": "atum:block/deadwood_stem", "x": 90, "y": 90 }},
    {   "when": { "facing": "up" },
        "apply": { "model": "atum:block/deadwood_stem", "x": 180 }},
    {   "when": { "facing": "down" },
        "apply": { "model": "atum:block/deadwood_stem" }}
  ]
}